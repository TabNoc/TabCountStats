{
  "name": "tabstats",
  "displayName": "TabStats",
  "version": "0.8.5",
  "private": true,
  "description": "Collects simple stats from the opend tabs, currently without any usage. Adds ability to open random Tab and hints the user to reduce Tab count.",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s lint clear build:web build:prepare build:js pack:zip",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:ts": "tsc --noEmit",
    "build:js": "npm run build:ts && vite build --config vite.config.content.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension/manifest.json extension/assets extension/dist -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf extension/dist extension/manifest.json extension.*",
    "lint": "eslint . --fix",
    "lint_org": "eslint '{src,scripts}/**/*.{json,ts,js,vue}'",
    "test": "vitest test"
  },
  "dependencies": {
    "liqe": "^3.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.7",
    "@ffflorian/jszip-cli": "^3.4.1",
    "@iconify/json": "^2.2.64",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.45",
    "@types/webextension-polyfill": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/compiler-sfc": "^3.3.2",
    "@vue/test-utils": "^2.3.2",
    "@vueuse/core": "^9.13.0",
    "autoprefixer": "^10.4.14",
    "chart.js": "^3.9.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "date-fns": "^2.30.0",
    "eslint": "^8.40.0",
    "esno": "^0.14.1",
    "flowbite": "^1.6.5",
    "flowbite-vue": "^0.0.10",
    "fs-extra": "^10.1.0",
    "jsdom": "^19.0.0",
    "kolorist": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.7.2",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^2.9.15",
    "vite-plugin-windicss": "^1.9.0",
    "vitest": "^0.9.4",
    "vue": "^3.3.2",
    "vue-chartjs": "^4.1.2",
    "vue-demi": "^0.12.5",
    "vue-router": "^4.2.0",
    "web-ext": "^7.6.2",
    "webext-bridge": "^5.0.5",
    "webextension-polyfill": "^0.9.0"
  }
}
