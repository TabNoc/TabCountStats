{
  "applications": {
    "gecko": {
      "strict_min_version": "79.0a1",
      "id": "TabStats@tabnoc.com"
    }
  },
  "background": {
    "page": "/src/html/background/background.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "TabAddonPage",
    "default_popup": "/src/html/addonButton/tabs.html"
  },
  "description": "Collects simple stats from the opend tabs, currently without any usage. Adds ability to open random Tab and hints the user to reduce Tab count.",
  "manifest_version": 2,
  "name": "TabStats",
  "options_ui": {
    "page": "/src/html/settings/settings.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "version": "0.7.2"
}